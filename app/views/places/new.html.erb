<h1>Add New Place</h1>
<%= form_with model: @place do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :picture %>
  <%= f.text_field :picture %>
  <%= f.submit %>
<% end %>
